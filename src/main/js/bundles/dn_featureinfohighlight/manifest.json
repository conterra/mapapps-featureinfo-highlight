{
  "Bundle-SymbolicName": "dn_featureinfohighlight",
  "Bundle-Version": "1.0.4",
  "Bundle-Name": "${bundleName}",
  "Bundle-Description": "${bundleDescription}",
  "Bundle-Vendor": "con terra GmbH",
  "Product-Name": "devnet-mapapps-featureinfo-highlight",
  "Bundle-Category": [
    "sample"
  ],
  "Bundle-Icon": {},
  "Bundle-License": {},
  "Require-Bundle": [
    {
      "name": "map",
      "version": "^3.4.0"
    },
    {
      "name": "featureinfo",
      "version": "^3.4.0"
    },
    {
      "name": "contentviewer",
      "version": "^3.4.0"
    }
  ],
  "Components": [
    {
      "name": "FeatureInfoHighlighter",
      "properties": {
      },
      "references": [
        {
          "name": "_drawGeometryHandler",
          "providing": "dn_featureinfoselector.DrawGeometryHandler"
        },
        {
          "name": "_contentViewer",
          "providing": "ct.contentviewer.ContentViewer"
        },
        {
          "name": "_infoViewer",
          "providing": "infoviewer.InfoViewer"
        }
      ]
    },
    {
      "name": "FeatureInfoController",
      "properties": {
      },
      "provides": [
      ],
      "references": [
      ]
    },
    {
      "name": "FeatureInfoWidget",
      "properties": {
      },
      "provides": [
      ],
      "references": [
      ]
    },
    {
      "name": "DrawGeometryHandler",
      "provides": "dn_featureinfoselector.DrawGeometryHandler",
      "properties": {
        "pointSymbolSize": 10
      },
      "references": [
        {
          "name": "_mapModel",
          "providing": "ct.mapping.map.MapModel"
        }
      ]
    },
    {
      "name": "ContentRegistration",
      "impl": "ct/Stateful",
      "provides": [
        "ct.contentviewer.ContentRule"
      ],
      "propertiesConstructor": true,
      "properties": {
        "contentRules": [
          {
            "matches": {
              "context": {
                "mapModelNodeId": "featureInfoHighlightGeometry"
              }
            },
            "skip": true
          }
        ]
      }
    }
  ]
}